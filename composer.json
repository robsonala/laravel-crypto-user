{
    "name": "robsonala/laravel-crypto-user",
    "version": "0.2.8",
    "description": "Cryptography tools for Laravel linked with User's table including Eloquent support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robson Alviani",
            "email": "robson.alviani@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "phpseclib/phpseclib": "2.*",
        "illuminate/database": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Robsonala\\CryptoUser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robsonala\\CryptoUser\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robsonala\\CryptoUser\\CryptoUserProvider"
            ]
        }
    }
}
